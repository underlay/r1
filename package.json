		"@storybook/react": "^5.0.11",
		"autoprefixer": "^9.5.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.5",
		"babel-plugin-macros": "^2.5.0",
		"babel-plugin-require-context-hook": "^1.0.0",
